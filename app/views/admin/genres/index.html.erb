<h1>Admin::Genres#index</h1>
<p>Find me in app/views/admin/genres/index.html.erb</p>

<p>ジャンル一覧・追加</p>

 <table>
  <tr>
  <%= form_with model: @genre, url: admin_genres_path do |f| %>
  <td>ジャンル名</td>
    <td><%= f.text_field :item_genre %></td>
    <td><%= f.submit '新規登録' %></td>
  <% end %>
  </tr>
 </table>

  <table>
   <tr>
    <th>ジャンル</th>
    <th></th>
   </tr>


    <% @genres.each do |genre| %>
    <tr>
    <td><%= genre.item_genre %></td>
    <td><%= link_to "編集する", edit_admin_genre_path(genre.id) %></td>
    </tr>
    <% end %>

  </table>